PROCEDURE dot_product(v1 ARRAY_OF INTEGER,v2 ARRAY_OF INTEGER)
    BEGIN
        FOR i FROM 0 TO v1.length STEP 1  DO
    ps:=ps+(v1[i]*v2[i])
        END_FOR
    END
ALGORITHM Dot product
VAR
ps INTEGER :=0;
    vector1 ARRAY_OF INTEGER [5];
    vector2 ARRAY_OF INTEGER [5];
BEGIN
    dot_product(vector1,vector2);
    ps==0?write("orthogonal vectors"):write("not orthogonal vectors")
   END
